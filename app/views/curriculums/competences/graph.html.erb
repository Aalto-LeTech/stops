

<% content_for :javascripts do %>
  <%= javascript_include_tag 'raphael-min' %>
  <%= javascript_include_tag 'views/curriculums/skillGraphView' %>
  <%= javascript_include_tag 'views/curriculums/graphCourse' %>
  <%= javascript_include_tag 'views/curriculums/graphSkill' %>
<% end %>


<% content_for :title do %>
  <%= @competence.localized_name + ' - ' + t('.title') %>
<% end %>


<%= render :partial => 'tabs' %>

<!--%= link_to t(:Back), curriculum_competence_path(:curriculum_id => @curriculum.id, :id => @competence.id) %-->

<div id="layers">
  <div id="svg"></div>
  <div id="course-graph"
    data-course-id="<%= @competence.id %>"
    data-courses-path="<%= curriculum_courses_path(:curriculum_id => @curriculum.id) %>"
    data-competences-path="<%= curriculum_competences_path(:curriculum_id => @curriculum.id) %>"
    data-skills-path="<%= curriculum_skills_path(:curriculum_id => @curriculum.id) %>">
  </div>
</div>
