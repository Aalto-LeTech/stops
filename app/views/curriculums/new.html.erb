<h1><%=t :new_curriculum_heading %></h1>

<% form_for(@curriculum) do |f| %>
  
  <table>
    <tr>
      <td><%= f.label :year, t(:curriculum_year) %></td>
      <td><%= f.text_field :year %></td>
      <td><%= f.hint :year %></td>
    </tr>
  </table>
  
  <p><%= f.submit t(:Upload) %></p>
<% end %>

<%= link_to t(:Back), curriculums_path %>