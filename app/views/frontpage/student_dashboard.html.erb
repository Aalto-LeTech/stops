<% content_for :javascript do %>
  <script type="text/javascript">
    <%= render "dashboard_i18n_strings.js" %>
  </script>
  <%= javascript_include_tag 'dashboard' %>
<% end %>


<h1><%= t('.heading') %></h1>

<p>
  <%= t('.welcome', name: @user.name) %>
  <span data-bind="text: $root.i18n.foo "></span>
  <span data-bind="text: $root.i18n.bar "></span>
</p>


<div id="plan" data-studyplan-path="<%= plan_path(@study_plan.id) %>">

  <%= render partial: 'schedule' %>

  <div class="row-fluid">
    <div class="span4">
      <%= render partial: 'competences' %>
    </div>
    <div class="span4">
      <%= render partial: 'courses' %>
    </div>
  </div>

  <div class="row-fluid">
    <div class="span4">
    </div>
    <div class="span4">
      <%= render :partial => 'curriculums' %>
    </div>
  </div>

</div>
