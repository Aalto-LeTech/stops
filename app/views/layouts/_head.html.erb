<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <title><%= content_for?(:title) ? yield(:title) : 'O4' %></title>
    <%= csrf_meta_tag %>
    <%= stylesheet_link_tag @stylesheets, :media => 'all' %>
