<!DOCTYPE html>
<html lang="<%= I18n.locale %>">
<% content_for(:corestylesheets) do -%>
  <%= stylesheet_link_tag "layouts/browser", :media => 'all' %>
<% end -%>
<%= render partial: 'layouts/core/head' %>

<body>
  <%= render partial: 'layouts/navs/top', locals: {fixed_nav: false}  %>

  <div class="affix-fluid">
    <div class="leftnav lower" data-spy="affix" data-offset-top="170">
      <div class="leftnav-content">
        <%= content_for(:leftnav) %>
      </div>
    </div>
  </div>
      
  <div class="affix-main-content container-fluid">
    <%= render partial: 'layouts/core/messages' %>
    <%= yield %>
  </div>

  <%= javascript_include_tag 'core/core' %>
  <%= yield(:javascripts) %>
</body>
</html>
