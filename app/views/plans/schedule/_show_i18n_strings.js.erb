/* Loads translated i18n strings used by dashboardView.js.coffee */
(function() {
  var i18n;

  /* i18n strings */
  i18n = {
    current_locale:                 "<%= I18n.locale %>",
    required_locales:               ["<%= REQUIRED_LOCALES.join('","').html_safe %>"],
    foo:                            "<%= escape_javascript(t('frontpage.foo')) %>",
    bar:                            "<%= escape_javascript(t('frontpage.bar')) %>"
  };

  /* Make translations available */
  window.i18n = i18n;
})();
